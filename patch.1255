+		 ],
+		 function( $ ) {
+	"use strict"; // jshint ;_;
+
+	/**
+	 * Initialize reCAPTCHA field if it hasn't been already.
+	 */
+	var init_recaptcha = function( $form ) {
+		var $recaptcha = $( '.ai1ec-recaptcha', $form );
