+		 ],
+		 function( ai1ec_config ) {
+	"use strict"; // jshint ;_;
+
+	// Get the language
+	var lang = ai1ec_config.language;
+	
+	// Create the url
+	var url = 'async!https://maps.google.com/maps/api/js?sensor=false&language=' + lang;
