+			date_time_utility_functions.ai1ec_apply_js_on_repeat_block
+		);
+
+		// handles click on exclude checkbox
+		date_time_utility_functions.ai1ec_click_on_checkbox(
+			'#ai1ec_exclude',
+			'#ai1ec_exclude_text > a',
